<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="1200.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gym_software.gym_software_third_project.controller.UserHomeController">
   <children>
      <VBox alignment="CENTER" layoutX="448.0" layoutY="130.0" prefHeight="339.0" prefWidth="303.0" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      
          <Button fx:id="btnPrenota" onAction="#handlePrenotazione" text="Prenota Lezione" />
          <Button fx:id="btnClienti" onAction="#handleEserciziTable" text="Visualizza esercizi" />
          <Button fx:id="modificaPwd" onAction="#handlePwdChange" text="Modifica Password" />
          <Button fx:id="btnLogout" onAction="#handleLogout" text="Logout" />
         <Button fx:id="GoBackButton" mnemonicParsing="false" onAction="#handleBackButtonClick" text="back" />
      </VBox>
   </children>
</AnchorPane>
